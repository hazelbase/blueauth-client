{
  "name": "blueauth-client",
  "version": "0.7.2",
  "description": "blueauth client",
  "engines": {
    "node": ">=10"
  },
  "main": "dist/main/index.js",
  "typings": "dist/module/index.d.ts",
  "module": "dist/module/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,graphql --exec 'npm run build'",
    "build": "graphql-codegen && npm run build:ts",
    "build:ts": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "prepublishOnly": "npm run build",
    "preversion": "npm run test",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "Adrian Artiles",
  "license": "MIT",
  "keywords": [
    "auth",
    "authentication",
    "client",
    "blueauth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kionin/blueauth-client.git"
  },
  "bugs": {
    "url": "https://github.com/kionin/blueauth-client/issues"
  },
  "homepage": "https://github.com/kionin/blueauth-client#readme",
  "files": [
    "dist/main",
    "dist/module",
    "src",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-graphql-request": "^3.2.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.12",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0"
  }
}
